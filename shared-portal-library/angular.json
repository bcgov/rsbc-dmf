{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shared-portal-lib": {
      "projectType": "library",
      "root": "projects/shared-portal-lib",
      "sourceRoot": "projects/shared-portal-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared-portal-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared-portal-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared-portal-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/shared-portal-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "partner-portal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/partner-portal",
      "sourceRoot": "projects/partner-portal/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/partner-portal",
            "index": "projects/partner-portal/src/index.html",
            "browser": "projects/partner-portal/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/partner-portal/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/partner-portal/src/favicon.ico",
              "projects/partner-portal/src/assets"
            ],
            "styles": [
              "projects/partner-portal/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "partner-portal:build:production"
            },
            "development": {
              "buildTarget": "partner-portal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "partner-portal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/partner-portal/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/partner-portal/src/favicon.ico",
              "projects/partner-portal/src/assets"
            ],
            "styles": [
              "projects/partner-portal/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
